@model Plenamente.Models.ZonaEmpresa

@{
    ViewBag.Title = "Delete";
    Layout = "~/Views/Shared/_LayoutAdministrador.cshtml";
}

<div class="colorVerdeBack">
    <p>Eliminar zona</p>
</div>

<div class="centrarContenido">
    <div>
        <hr />
        <dl class="dl-horizontal">

            <dt>
                Nombre zona
            </dt>

            <dd>
                @Html.DisplayFor(model => model.Zemp_Nom)
            </dd>
        </dl>

        @using (Html.BeginForm())
        {
            @Html.AntiForgeryToken()

            <div class="form-actions no-color">
                <br />
                <br />
                <input type="submit" value="Borrar" class="btn btn-default" />
                @Html.ActionLink("Back to List", "Index")
            </div>
        }
    </div>
    <div class="contenidoVolver">
        <a href="@Url.Action("Index", "zonaEmpresas")" class="ayudaTitulos"><span>Volver a la lista</span> <img src="~/Imagenes/flecha-hacia-la-izquierda.png" /><p></p></a>
    </div>
