@model Plenamente.Models.ObjEmpresa

@{
    ViewBag.Title = "Delete";
    Layout = "~/Views/Shared/_LayoutAdministrador.cshtml";
}

<div class="colorVerdeBack">
    <p>Borrar objetivo de la empresa</p>
</div>
<div class="centrarContenido">
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Nombre
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Oemp_Nombre)
        </dd>

        <dt>
            Descripción
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Oemp_Descrip)
        </dd>



    </dl>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <br />
            <br />
            <input type="submit" value="Borrar" class="btn btn-default" />
            @Html.ActionLink("Back to List", "Index")
        </div>
    }
</div>

<div class="contenidoVolver">
    <a href="@Url.Action("Index", "ObjEmpresas")" class="ayudaTitulos"><span>Volver a la lista</span> <img src="~/Imagenes/flecha-hacia-la-izquierda.png" /><p></p></a>
</div>
