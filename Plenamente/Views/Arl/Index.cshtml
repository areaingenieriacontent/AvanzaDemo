@model IEnumerable<Plenamente.Models.Arl>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutAdministrador.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Arl_Nom)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Arl_Registro)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Arl_Nom)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Arl_Registro)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Arl_Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Arl_Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Arl_Id })
        </td>
    </tr>
}

</table>
