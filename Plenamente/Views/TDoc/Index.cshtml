@model IEnumerable<Plenamente.Models.TipoDocumento>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutAdministrador.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Tdoc_Nom)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Tdoc_Registro)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Tdoc_Nom)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Tdoc_Registro)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Tdoc_Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Tdoc_Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Tdoc_Id })
        </td>
    </tr>
}

</table>
