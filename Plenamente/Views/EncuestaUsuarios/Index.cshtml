@model IEnumerable<Plenamente.Models.Encuesta>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutUsuario.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Empresa.Empr_Nom)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Encu_Nombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Encu_Creacion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Encu_Vence)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Encu_Estado)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Encu_Registro)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Empresa.Empr_Nom)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Encu_Nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Encu_Creacion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Encu_Vence)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Encu_Estado)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Encu_Registro)
        </td>
        <td>
            @Html.ActionLink("Edit", "Encuesta", new { id=item.Encu_Id }) |
        </td>
    </tr>
}

</table>
